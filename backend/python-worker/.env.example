# IMPORTANT: Copy this to .env and fill in your actual credentials
# NEVER commit your .env file to git!

# OpenRouter / Claude
OPENROUTER_API_KEY=your_openrouter_api_key_here
CLAUDE_MODEL=anthropic/claude-sonnet-4.5
OPENROUTER_URL=https://openrouter.ai/api/v1/chat/completions

# Supabase
SUPABASE_URL=your_supabase_project_url_here
SUPABASE_SERVICE_KEY=your_supabase_service_role_key_here

# MongoDB
MONGODB_URI=your_mongodb_connection_string_here
MONGODB_DB=codeinsight

# Redis (Upstash)
REDIS_URL=your_redis_url_here

# Token Budgets (optional, defaults in config.py)
TOKEN_BUDGET_LITE=200000
TOKEN_BUDGET_PRO=500000
TOKEN_BUDGET_BUSINESS=4000000
